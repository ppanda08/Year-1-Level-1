#include <stdio.h>

int main(void)
{
	int age = 19;

	printf("Prefix:  %d\n", ++age);
	printf("         %d\n", age);
	printf("Postfix: %d\n", age++);
	printf("         %d\n", age);
	printf("         %d\n", ++age);

	return 0;
}

/*OUTPUTS
Prefix:  20
         20
Postfix: 20
         21
         22 
          */
